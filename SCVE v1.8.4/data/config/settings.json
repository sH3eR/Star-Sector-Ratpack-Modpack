{
    # SHIP FILTERS
    # 0 = block all spoilers
    # 1 = block heavy spoilers
    # 2 = block no spoilers,
    # Default 0
    "SCVE_defaultShipFilter": 0,

    # WEAPON FILTERS
    # 0 = block all weapons not from the mod
    # 1 = block all mod weapons not from the mod
    # 2 = default (block restricted weapons)
    # 3 = allow all weapons
    # 4 = custom filter
    # Default 2
    "SCVE_defaultWeaponWingFilter": 2,

    # HULLMOD FILTERS
    # 0 - default
    # 1 - show s-mods
    # 2 - show d-mods
    # 3 - show all hullmods,
    # 4 - show custom hullmods
    # Default 0
    "SCVE_defaultHullModFilter": 0,
}